    <entry>
      <name>Redshift-S3-Dump</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>COPY date
FROM 's3://pentaho-in/date.csv'

-- Redshift authorization to access S3 buckets
-- Add Redshift-S3 access role in AWS IAM-console
IAM_ROLE 'arn:aws:iam::00000000000:role/RedshiftFullAccessToS3'
DELIMITER ','
FILLRECORD
IGNOREHEADER 1
REGION 'us-east-2'
;

COPY product
FROM 's3://pentaho-in/product.csv'
-- Redshift authorization to access S3 buckets
-- Add Redshift-S3 access role in AWS IAM-console
IAM_ROLE 'arn:aws:iam::00000000000:role/RedshiftFullAccessToS3'
DELIMITER ','
FILLRECORD
IGNOREHEADER 1
REGION 'us-east-2'
;

COPY sales
FROM 's3://pentaho-in/sales.csv.gz'
-- Redshift authorization to access S3 buckets
-- Add Redshift-S3 access role in AWS IAM-console
IAM_ROLE 'arn:aws:iam::00000000000:role/RedshiftFullAccessToS3'
DELIMITER ','
FILLRECORD
GZIP
IGNOREHEADER 1
REGION 'us-east-2'
;
</sql>
